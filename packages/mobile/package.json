{
  "name": "@lyricnote/mobile",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "predev": "cd ../shared && pnpm build",
    "dev": "expo start",
    "prestart": "cd ../shared && pnpm build",
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "ios:dev": "expo run:ios",
    "android:dev": "expo run:android",
    "web": "expo start --web",
    "prebuild": "cd ../shared && pnpm build",
    "build": "tsc --noEmit",
    "build:app": "expo build",
    "dev:build:ios": "eas build --profile development --platform ios --local",
    "dev:build:android": "eas build --profile development --platform android --local",
    "dev:build:ios:simulator": "eas build --profile development --platform ios --local",
    "dev:build:android:emulator": "eas build --profile development --platform android --local",
    "dev:cloud:ios": "eas build --profile development --platform ios",
    "dev:cloud:android": "eas build --profile development --platform android",
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@lyricnote/shared": "file:../shared",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/bottom-tabs": "^7.4.7",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/stack": "^7.4.8",
    "eas-cli": "^16.24.1",
    "expo": "54.0.19",
    "expo-av": "^16.0.7",
    "expo-dev-client": "^6.0.16",
    "expo-file-system": "^19.0.17",
    "expo-status-bar": "~3.0.8",
    "nativewind": "^4.2.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "^4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.0",
    "react-native-worklets": "^0.5.1",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@types/react": "~19.1.10",
    "babel-preset-expo": "^54.0.2",
    "typescript": "~5.9.2"
  },
  "private": true
}
