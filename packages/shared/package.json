{
  "name": "@lyricnote/shared",
  "version": "1.0.0",
  "description": "Shared types, constants, and utilities for LyricNote",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./analytics/server": {
      "types": "./dist/analytics/server/index.d.ts",
      "import": "./dist/analytics/server/index.js",
      "default": "./dist/analytics/server/index.js"
    },
    "./analytics/components": {
      "types": "./dist/analytics/components/index.d.ts",
      "import": "./dist/analytics/components/index.js",
      "default": "./dist/analytics/components/index.js"
    },
    "./universalExport": {
      "types": "./dist/universalExport/index.d.ts",
      "import": "./dist/universalExport/index.js",
      "default": "./dist/universalExport/index.js"
    },
    "./universalFile": {
      "types": "./dist/universalFile/index.d.ts",
      "import": "./dist/universalFile/index.js",
      "default": "./dist/universalFile/index.js"
    },
    "./i18n/web": {
      "types": "./dist/i18n/web.d.ts",
      "import": "./dist/i18n/web.js",
      "default": "./dist/i18n/web.js"
    },
    "./i18n/components/LanguageSwitcher.css": "./src/i18n/components/LanguageSwitcher.css"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "swr": "^2.3.6"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "~18.2.43",
    "typescript": "^5.0.0",
    "drizzle-orm": "^0.44.5"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "typescript": "^5.0.0"
  }
}
