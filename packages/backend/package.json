{
  "name": "@lyricnote/backend",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "predev": "cd ../shared && pnpm build",
    "dev": "next dev",
    "build": "NODE_ENV=production next build",
    "start": "next start",
    "lint": "eslint",
    "db:dev:generate-push": "dotenv -e .env.local -- npx drizzle-kit generate && dotenv -e .env.local -- npx drizzle-kit push",
    "db:pro:generate-push": "dotenv -e .env.production -- npx drizzle-kit generate && dotenv -e .env.production -- npx drizzle-kit push",
    "db:create-admin": "dotenv -e .env.local -- npx tsx scripts/create-admin.ts",
    "mobile:dev": "pnpm --filter @lyricnote/mobile start",
    "miniapp:dev": "pnpm --filter @lyricnote/miniapp dev"
  },
  "dependencies": {
    "@lyricnote/shared": "file:../shared",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@types/pg": "^8.15.5",
    "autoprefixer": "^10.4.21",
    "bcryptjs": "^3.0.2",
    "better-auth": "^1.3.16",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-kit": "^0.31.5",
    "drizzle-orm": "^0.44.5",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "kuroshiro": "^1.2.0",
    "kuroshiro-analyzer-mecab": "^1.0.1",
    "lucide-react": "^0.544.0",
    "multer": "^2.0.2",
    "next": "15.5.3",
    "next-auth": "^4.24.11",
    "pg": "^8.16.3",
    "postcss": "^8.5.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "redis": "^5.8.2",
    "swr": "^2.3.6",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.13",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv-cli": "^7.3.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "tsx": "^4.20.5",
    "typescript": "^5"
  }
}
